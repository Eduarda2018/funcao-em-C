#include <stdio.h>

#include <stdlib.h>

int verif (int num1, int num2){

	if(num1 > num2)

	return num1;

	else

	return num2;

	}

	int main(){

int num1 =10;

	int num2 =29;

	while(num1 == num2){

	printf("Erro!. os numeros devem ser diferentes: Digite outro numero:");

	scanf("%d",&num2);

	}

	printf("O maior numero e: %d\n",verif(num1,num2));

	}
